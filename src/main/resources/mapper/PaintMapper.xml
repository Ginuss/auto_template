<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gy.mapper.PaintMapper">
    <resultMap id="BaseResultMap" type="com.gy.pojo.Paint">
        <id column="age" jdbcType="INTEGER" property="age" />
        <result column="blood" jdbcType="INTEGER" property="blood" />
    </resultMap>

    <select id="getData" resultMap="BaseResultMap">
        select age,blood from relation
    </select>
</mapper>

